<templateSet group="unit test">
  <template name="that_fest" value="org.fest.assertions.api.Assertions.assertThat($OBJECT$)" description="that / fest assert" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="OBJECT" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ext" value="org.assertj.core.api.Assertions.extractProperty($PROPERTY$).from($ITERABLE$)" description="extracte property" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="PROPERTY" expression="&quot;&quot;" defaultValue="" alwaysStopAt="true" />
    <variable name="ITERABLE" expression="variableOfType(Type)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="that" value="org.assertj.core.api.Assertions.assertThat($OBJECT$)" description="that / assert4j" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="OBJECT" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ext_fest" value="org.fest.assertions.api.Assertions.extractProperty($PROPERTY$).from($ITERABLE$)" description="extracte property / fest assert" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="PROPERTY" expression="&quot;&quot;" defaultValue="" alwaysStopAt="true" />
    <variable name="ITERABLE" expression="variableOfType(Type)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="filter" value="org.assertj.core.api.Assertions.filter($ITERABLE$).with($FIELD$, $VALUE$).get()" description="filter on properties" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="ITERABLE" expression="variableOfType(Type)" defaultValue="" alwaysStopAt="true" />
    <variable name="FIELD" expression="&quot;&quot;" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>